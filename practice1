void main() {
  bool isTrue = true;
     
  int num2 = 100;
  int num3 = 100;
  
  double num4 = 3.14;
  
  String string = 'Hello World';
  
  // null 값만 줄 수 있음.
  Null thisIsNull = null;
    
    // 노란줄 == 선언만 하고 할당하지 않았을 경우
    
    print(isTrue);
    print(num2);
    print(num3);
    print(string);
  print(thisIsNull);
  
 }
  
